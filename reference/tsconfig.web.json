{
  "extends": "./tsconfig.app.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.web.tsbuildinfo",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/renderer/src/*"],
      "@framework/*": ["src/renderer/WebSDK/Framework/src/*"],
      "@cubismsdksamples/*": ["src/renderer/WebSDK/src/*"],
      "@motionsyncframework/*": ["src/renderer/WebSDK/src/Framework/src/*"],
      "@motionsync/*": ["src/renderer/MotionSync/motionsync/*"]
    }
  },
  "include": [
    "src/renderer/**/*.ts",
    "src/renderer/**/*.tsx",
    "src/renderer/**/*.d.ts",
    "src/renderer/WebSDK/src/**/*"
  ]
}
